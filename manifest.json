{
  "name": "Pseudorandom Actions",
  "description": "Get a random to do action from a list with a history which affect to the chance.",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "actions-popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon32.png"
    }
  },
  "background": {
    "service_worker": "contextMenus.js"
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon32.png"
  },
  "permissions": ["storage", "activeTab", "scripting", "contextMenus"]
}